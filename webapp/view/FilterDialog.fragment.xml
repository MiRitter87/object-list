<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm="onFilterDialogConfirm">
		<filterItems>
			<ViewSettingsFilterItem	text="{i18n>object.range}" key="range" multiSelect="false">
				<items>
					<ViewSettingsItem text="{i18n>object.range.one}" key="range_EQ_100-199" />
					<ViewSettingsItem text="{i18n>object.range.two}" key="range_EQ_200-299" />
					<ViewSettingsItem text="{i18n>object.range.three}" key="range_EQ_300-399" />
				</items>
			</ViewSettingsFilterItem>
			
			<ViewSettingsFilterItem	text="{i18n>object.attribute1}" key="attribute1" multiSelect="false">
				<items>
					<ViewSettingsItem text="A" key="attribute1_EQ_A" />
					<ViewSettingsItem text="B" key="attribute1_EQ_B" />
					<ViewSettingsItem text="C" key="attribute1_EQ_C" />
					<ViewSettingsItem text="D" key="attribute1_EQ_D" />
					<ViewSettingsItem text="E" key="attribute1_EQ_E" />
					<ViewSettingsItem text="F" key="attribute1_EQ_F" />
					<ViewSettingsItem text="G" key="attribute1_EQ_G" />
					<ViewSettingsItem text="H" key="attribute1_EQ_H" />
				</items>
			</ViewSettingsFilterItem>
			
			<ViewSettingsFilterItem	text="{i18n>object.attribute3}" key="attribute3" multiSelect="false">
				<items>
					<ViewSettingsItem text="{i18n>object.attribute3.micro}" key="attribute3_EQ_micro" />
					<ViewSettingsItem text="{i18n>object.attribute3.mini}" key="attribute3_EQ_mini" />
					<ViewSettingsItem text="{i18n>object.attribute3.midi}" key="attribute3_EQ_midi" />
					<ViewSettingsItem text="{i18n>object.attribute3.big}" key="attribute3_EQ_big" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>