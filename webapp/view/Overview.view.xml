<!DOCTYPE xml>
<mvc:View
	controllerName="object-list.controller.Overview"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	
	<Page title="{i18n>overview.header}">
		<content>
			<Table id="objectTable">
				<headerToolbar>
					<OverflowToolbar>
						<Title text="{i18n>overview.objects}"/>
					</OverflowToolbar>
				</headerToolbar>
				
				<columns>
					<Column minScreenWidth="Desktop" demandPopin="true">
						<Text text="{i18n>object.id}"/>
					</Column>
					<Column minScreenWidth="Desktop" demandPopin="true">
						<Text text="{i18n>object.range}"/>
					</Column>
					<Column minScreenWidth="Desktop" demandPopin="true">
						<Text text="{i18n>object.number1}"/>
					</Column>
				</columns>
				
				<items>
					<ColumnListItem>
						<cells>
							<ObjectIdentifier text="{objects>id}"/>
							<Text text="{objects>range}"/>
							<Text text="{objects>number1}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>